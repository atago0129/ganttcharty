{
  "name": "ganttcharty",
  "version": "1.0.0",
  "description": "simple ganttchart library.",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config=webpack.config.build.js",
    "debug": "webpack-dev-server --config=webpack.config.debug.js",
    "test": "jest",
    "watch": "run-p watch:*",
    "watch:test": "jest --watch",
    "watch:build": "webpack --watch",
    "prettier": "prettier --write './{src,test}/**/*.{ts,tsx}'",
    "pretty-quick": "pretty-quick",
    "precommit": "pretty-quick --staged",
    "postcommit": "git reset"
  },
  "author": "",
  "license": "ISC",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json",
        "diagnostics": true
      }
    },
    "testMatch": [
      "<rootDir>/(src|test)/**/*.test.+(ts|tsx|js)"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "devDependencies": {
    "@types/classnames": "^2.2.7",
    "@types/date-fns": "^2.6.0",
    "@types/jest": "^23.3.7",
    "@types/lodash": "^4.14.117",
    "@types/memoize-one": "^3.1.1",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "@types/react-icons": "^2.2.6",
    "@types/react-resize-detector": "^3.1.0",
    "@types/styled-components": "^4.0.2",
    "@types/uuid": "^3.4.4",
    "husky": "^1.1.2",
    "jest": "^23.6.0",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.3",
    "pretty-quick": "^1.8.0",
    "snapshot-diff": "^0.4.0",
    "ts-jest": "^23.10.4",
    "ts-loader": "^5.2.1",
    "typescript": "^3.1.1",
    "webpack": "^4.23.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "closest-element": "0.0.3",
    "date-fns": "^1.29.0",
    "hotkeys-js": "^3.3.8",
    "lodash": "^4.17.11",
    "memoize-one": "^4.0.2",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-icons": "^3.2.2",
    "react-resize-detector": "^3.1.3",
    "srimmer": "^0.6.0",
    "styled-components": "^4.0.0-beta.11.3",
    "tslib": "^1.9.3",
    "uuid": "^3.3.2"
  }
}
